{
  "name": "ui",
  "version": "0.1.17",
  "repository": {
    "type": "git",
    "url": "ssh://git@crgitep:7999/ap/front-end.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "ssl": "ng serve --ssl true --ssl-cert 'ssl/server.crt' --ssl-key 'ssl/server.key'",
    "build": "ng build",
    "build:prod": "ng build --prod --build-optimizer --aot",
    "test": "ng test --code-coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "integration:test": "PORT=8080 node scripts/run-cypress-test",
    "integration:test:dev": "PORT=8080 NODE_ENV=DEV node scripts/run-cypress-test",
    "icon-gen": "rm -rf ./src/assets/favicon && icon-gen -i ./src/assets/favicon.svg -t svg -o ./src/assets/favicon -r"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@types/rx-dom": "^7.0.0",
    "core-js": "^2.5.4",
    "d3": "^5.3.0",
    "d3-selection-multi": "^1.0.1",
    "fs-extra": "^6.0.1",
    "moment": "^2.22.1",
    "normalize-scss": "^7.0.1",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.1.0",
    "zone.js": "^0.8.26"
