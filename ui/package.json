    "@angular/language-service": "^6.0.0",
    "@cypress/webpack-preprocessor": "^2.0.1",
    "@types/d3": "^5.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/moment": "^2.13.0",
    "@types/node": "~8.9.4",
    "chalk": "^2.4.1",
