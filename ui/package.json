    "@angular/language-service": "7.0.1",
    "@cypress/webpack-preprocessor": "3.0.1",
    "@types/d3": "5.0.1",
    "@types/jasmine": "2.8.9",
    "@types/jasminewd2": "2.0.5",
    "@types/moment": "^2.13.0",
    "@types/node": "10.12.0",
    "chalk": "^2.4.1",
    "codelyzer": "4.5.0",
    "coveralls": "3.0.2",
    "cypress": "3.1.0",
    "eyes.images": "0.0.53",
    "icon-gen": "1.2.3",
