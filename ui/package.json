{
  "name": "ui",
  "version": "0.1.19",
  "repository": {
    "type": "git",
    "url": "ssh://git@crgitep:7999/ap/front-end.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "ssl": "ng serve --ssl true --ssl-cert 'ssl/server.crt' --ssl-key 'ssl/server.key'",
    "build": "ng build",
    "build:prod": "ng build --prod --build-optimizer --aot",
    "test": "ng test --watch=false --code-coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "integration:test": "PORT=8080 node scripts/run-cypress-test",
    "integration:test:dev": "PORT=8080 NODE_ENV=DEV node scripts/run-cypress-test",
    "icon-gen": "rm -rf ./src/assets/favicon && icon-gen -i ./src/assets/favicon.svg -t svg -o ./src/assets/favicon -r"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.0.1",
    "@angular/common": "7.0.1",
    "@angular/compiler": "7.0.1",
    "@angular/core": "7.0.1",
    "@angular/forms": "7.0.1",
    "@angular/http": "7.0.1",
    "@angular/platform-browser": "7.0.1",
    "@angular/platform-browser-dynamic": "7.0.1",
    "@angular/router": "7.0.1",
    "@types/rx-dom": "^7.0.0",
    "core-js": "2.5.7",
    "d3": "5.7.0",
    "d3-selection-multi": "^1.0.1",
    "fs-extra": "7.0.0",
    "moment": "^2.22.1",
    "normalize-scss": "^7.0.1",
    "rxjs": "6.3.3",
    "rxjs-compat": "6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.10.3",
